{
  "scriptFile" : "..\\mesmedicaments-1.2-SNAPSHOT.jar",
  "entryPoint" : "app.mesmedicaments.PublicTriggers.connexion",
  "bindings" : [ {
    "type" : "httpTrigger",
    "direction" : "in",
    "name" : "connexionTrigger",
    "route" : "api/connexion/{etape:int}",
    "methods" : [ "POST" ],
    "dataType" : "string",
    "authLevel" : "ANONYMOUS"
  }, {
    "type" : "queue",
    "direction" : "out",
    "name" : "connexionQueueOutput",
    "queueName" : "nouvelles-connexions",
    "connection" : "AzureWebJobsStorage"
  }, {
    "type" : "http",
    "direction" : "out",
    "name" : "$return"
  } ]
}